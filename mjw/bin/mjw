#!/usr/bin/env python3

import os
import subprocess
import sys
import importlib.util

parent_path = os.path.dirname(os.path.dirname(__file__))
spec = importlib.util.spec_from_file_location("path", os.path.join(parent_path, "path.py"))
path = importlib.util.module_from_spec(spec)
spec.loader.exec_module(path)

subprocess.run([path.getPackagesPath("mjw-cli", "env", "bin", "mjw"), *sys.argv[1:]])
