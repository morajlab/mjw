#!/usr/bin/env bash

export MJW_ROOT_PATH=$(dirname $(dirname $0))
export MJW_COMMANDS_PATH="$MJW_ROOT_PATH/scripts/commands"
export MJW_SOURCE_PATH="$MJW_ROOT_PATH/src"
export MJW_PACKAGES_PATH="$MJW_ROOT_PATH/packages"
export MJW_CACHE_PATH="$MJW_ROOT_PATH/.cache"
export MJW_NODE_PATH="$MJW_ROOT_PATH/node_modules"
export MJW_NODE_BIN_PATH="$MJW_NODE_PATH/.bin"

# Load commands
source "$MJW_ROOT_PATH/scripts/commands.sh"

commands $*
