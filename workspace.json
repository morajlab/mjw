{
  "version": 2,
  "projects": {
    "my-blog-my-blog": {
      "root": "apps/my-blog/my-blog",
      "sourceRoot": "apps/my-blog/my-blog",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/my-blog/my-blog",
            "outputPath": "dist/apps/my-blog/my-blog"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "my-blog-my-blog:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "my-blog-my-blog:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "my-blog-my-blog:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/my-blog/my-blog"],
          "options": {
            "jestConfig": "apps/my-blog/my-blog/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/my-blog/my-blog/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "my-blog-my-blog-e2e": {
      "root": "apps/my-blog/my-blog-e2e",
      "sourceRoot": "apps/my-blog/my-blog-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/my-blog/my-blog-e2e/cypress.json",
            "devServerTarget": "my-blog-my-blog:serve",
            "tsConfig": "apps/my-blog/my-blog-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "my-blog-my-blog:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/my-blog/my-blog-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "my-blog-my-blog"
}
